{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "camelcase": "off",
      "import/no-named-as-default-member":"off",
      "import/no-named-as-default":"off",
      "prettier/prettier": "error",
      "no-unused-vars":"warn",
      "import/extensions":[
        "error", {"js":"always"}
      ]
    }
}
